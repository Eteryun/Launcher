{
  "name": "eteryun-launcher",
  "productName": "Eteryun Launcher",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Eteryun Launcher",
  "author": "Ramon Rodrigues",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "crypto": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.65",
    "@electron-forge/publisher-github": "^6.0.0-beta.66",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@svgr/webpack": "^6.3.1",
    "@types/adm-zip": "^0.5.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "dayjs": "^1.11.5",
    "electron": "^20.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "polished": "^4.2.2",
    "prettier": "^2.7.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "eteryun-launcher",
        "executableName": "eteryun-launcher",
        "icon": "assets/icon",
        "asar": true,
        "extraResource": [
          "assets"
        ],
        "win32metadata": {
          "ProductName": "Eteryun Launcher"
        }
      },
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "devContentSecurityPolicy": "default-src * self blob: data: gap:; style-src * self 'unsafe-inline' blob: data: gap:; script-src * 'self' 'unsafe-eval' 'unsafe-inline' blob: data: gap:; object-src * 'self' blob: data: gap:; img-src * self 'unsafe-inline' blob: data: gap:; connect-src self * 'unsafe-inline' blob: data: gap:; frame-src * self blob: data: gap:;",
            "renderer": {
              "nodeIntegration": true,
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/main/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Eteryun",
              "name": "Launcher"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Eteryun-Launcher",
            "setupIcon": "assets/icon.ico",
            "iconUrl": "https://raw.githubusercontent.com/Eteryun/Launcher/master/assets/icon.ico",
            "title": "Eteryun Launcher"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
